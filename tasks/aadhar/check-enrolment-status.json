{
    "title": "Check enrolment status | check enrolment status in aadhar | check aadhar enrolment status | check erolment update status",
    "selectorAttribute": "data-testid",
    "steps": [
        
        {
            "type": "click",
            "target": "main",
            "label": "Click here to check enrolment status",
            "allowNavigation": true,
            "selectors": [
                [
                    "div[role='button'][aria-label='Check  enrolment or update status']"
                  ]
            ]  
        },
        
        {
            "type": "click",
            "target": "main",
            "label": "Choose an option to proceed",
            "selectors": [   
               [
                    ".auth-form__radio-group"
                ]    
            ]
        },
        
        {
            "type": "input",
            "target": "main",
            "label": "Enter 14 digit Enrolment Number",
            "selectors": [
               [
                    "form > div > div:nth-of-type(2) input"
                ]       
            ] 
        },
        
         {
            "type": "input",
            "target": "main",
            "label": "Select Date",
            "selectors": [  
              [
                    "#calender"
                ]       
           ]       
        },
        {
            "type": "input",
            "target": "main",
            "label": "Select Time",
            "selectors": [
                [
                    "#timepick"
                ]               
            ]  
        },
        {
            "type": "input",
            "target": "main",
            "label": "Enter captcha",
            "selectors": [
                [
                     "div.aadhaar-form__captcha-field-container input"
                ]       
            ]
        },
        {
            "type": "click",
            "target": "main",
            "label": "Click to submit",
            "selectors": [
                [
                     "div.aadhaar-form__button-container > div"
                ]     
            ]
        } 
    ]
}