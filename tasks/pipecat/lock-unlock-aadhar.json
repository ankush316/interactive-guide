{
    "title": "How to lock or unlock aadhar | lock aadhar | unlock aadhar | aadhar lock | aadhar unlock",
    "selectorAttribute": "data-testid",
    "steps": [
        {
            "type": "click",
            "target": "main",
            "label": "Click here to lock or unlock aadhar",
            "selectors": [
                [
                    "div[role='button'][aria-label='Lock / Unlock Aadhaar']"    
                ]
            ]  
        }, 
        {
            "type": "click",
            "target": "main",
            "label": "Click on next button",
            "selectors": [
                [
                   "div.form-buttons__container > div"
                ]
            ]  
        }, 
        {
            "type": "click",
            "target": "main",
            "label": "Select an option to Lock or Unlock Aadhaar",
            "selectors": [
                [
                    "div.lock-unlock-aadhaar-form__radio-group > div:nth-of-type(1)"
                ]
            ]  
        }, 
        {
            "type": "input",
            "target": "main",
            "label": "Enter Virtual ID (VID)",
            "selectors": [
                [
                    "div.lock-unlock-aadhaar-form__form-container > div.sc-fXSgeo input"
                ]
            ]  
        }, 
        {
            "type": "input",
            "target": "main",
            "label": "Enter Full Name",
            "selectors": [
                [
                   "div.lock-unlock-aadhaar-form__form-container > div:nth-of-type(2) input"
                ]
            ]  
        }, 
        {
            "type": "input",
            "target": "main",
            "label": "Enter Pincode",
            "selectors": [
                [
                    "div.lock-unlock-aadhaar-form__form-container"
                ]
            ]  
        }, 
        {
            "type": "input",
            "target": "main",
            "label": "Enter Captcha",
            "selectors": [
                [
                    "div.lock-unlock-aadhaar-form__captcha-field > div > div > div"
                ]
            ]  
        }, 
        {
            "type": "click",
            "target": "main",
            "label": "Click here to send OTP",
            "selectors": [
                [
                    "#root button"
                ]
            ]  
        },
        {
            "type": "input",
            "target": "main",
            "label": "Enter Verification code that you have received on your registered mobile number",
            "selectors": [
                [
                    "#root div:nth-of-type(3) input"
                ]
            ]  
        },
        {
            "type": "click",
            "target": "main",
            "label": "Click here to login",
            "selectors": [
                [
                     "#root button"
                ]
            ]  
        }
       
    ]
}